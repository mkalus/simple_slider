<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
		<!--  <langChildren>1</langChildren>!-->
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:simple_slider/locallang.xml:ff_general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<templateFile>
						<TCEforms>
							<label>LLL:EXT:simple_slider/locallang.xml:ff_templateFile</label>
							<config>
								<type>input</type>
							</config>
						</TCEforms>
					</templateFile>
					<animation>
						<TCEforms>
							<label>LLL:EXT:simple_slider/locallang.xml:ff_animation</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">slide</numIndex>
										<numIndex index="1">slide</numIndex>
									</numIndex>					
									<numIndex index="1" type="array">
										<numIndex index="0">fade</numIndex>
										<numIndex index="1">fade</numIndex>
									</numIndex>
								</items>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</animation>
					<animationSpeed>
						<TCEforms>
							<label>LLL:EXT:simple_slider/locallang.xml:ff_animationSpeed</label>
							<config>
								<type>input</type>
								<eval>num</eval>
								<size>20</size>
								<checkbox></checkbox>
							</config>
						</TCEforms>
					</animationSpeed>
					<slidePause>
						<TCEforms>
							<label>LLL:EXT:simple_slider/locallang.xml:ff_slidePause</label>
							<config>
								<type>input</type>
								<eval>num</eval>
								<size>20</size>
								<checkbox></checkbox>
							</config>
						</TCEforms>
					</slidePause>
					<imageWidth>
						<TCEforms>
							<label>LLL:EXT:simple_slider/locallang.xml:ff_imageWidth</label>
							<config>
								<type>input</type>
								<eval>num</eval>
								<size>20</size>
								<checkbox></checkbox>
							</config>
						</TCEforms>
					</imageWidth>
					<imageHeight>
						<TCEforms>
							<label>LLL:EXT:simple_slider/locallang.xml:ff_imageHeight</label>
							<config>
								<type>input</type>
								<eval>num</eval>
								<size>20</size>
								<checkbox></checkbox>
							</config>
						</TCEforms>
					</imageHeight>
					<storePid>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:simple_slider/locallang.xml:ff_storePid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</storePid>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>